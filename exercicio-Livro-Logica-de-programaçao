    <script>
     const valorConta = Number(prompt("Digite o valor da conta:"))
     const numCliente = Number(prompt("Quantas pessoas vão pagar a conta?:"))
     const valorCliente = valorConta / numCliente
     alert(`O valor dividido para cada um é: ${valorCliente}`)










    </script>
    <title>Document</title>
